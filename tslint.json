{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "interface-over-type-literal": false,
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "static-method",
          "instance-field",
          "constructor",
          "instance-method"
        ]
      }
    ],
    "object-literal-sort-keys": false
  }
}
